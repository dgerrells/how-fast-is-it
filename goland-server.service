[Unit]
Description=Gopher Service
After=network.target

[Service]
WorkingDirectory=/root/how-fast-is-it/go-land/
ExecStart=/root/how-fast-is-it/go-land/server
StandardOutput=append:/var/log/goland/out.log
StandardError=append:/var/log/goland/err.log
Restart=always
User=root
Type=simple
RestartSec=3

[Install]
WantedBy=multi-user.target